<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TitledPane alignment="CENTER" animated="false" collapsible="false" prefHeight="400.0" prefWidth="600.0" text="Team info">
         <content>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <GridPane prefHeight="177.0" prefWidth="379.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="183.0" minWidth="10.0" prefWidth="134.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="285.0" minWidth="10.0" prefWidth="235.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="52.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="82.0" minHeight="10.0" prefHeight="82.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label prefHeight="17.0" prefWidth="83.0" text="Team name" />
                        <Label fx:id="teamNameLabel" text="TEAM NAME" GridPane.columnIndex="1">
                           <GridPane.margin>
                              <Insets left="10.0" />
                           </GridPane.margin>
                        </Label>
                        <Label text="Team contact person" GridPane.rowIndex="1" />
                        <Label text="Team members" GridPane.rowIndex="2" />
                        <Label fx:id="contactPersonLabel" text="CONTACT PERSON" GridPane.columnIndex="1" GridPane.rowIndex="1">
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <GridPane.margin>
                              <Insets left="10.0" />
                           </GridPane.margin>
                        </Label>
                        <Label fx:id="teamMembersLabel" text="MEMBERS" GridPane.columnIndex="1" GridPane.rowIndex="2">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="10.0" top="5.0" />
                     </padding>
                  </GridPane>
                  <ImageView fx:id="cakeImageView" fitHeight="201.0" fitWidth="238.0" layoutX="360.0" layoutY="27.0" pickOnBounds="true" smooth="false" />
               </children>
            </Pane>
         </content></TitledPane>
   </children>
</AnchorPane>
